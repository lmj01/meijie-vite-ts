{
  "name": "meijie-vite-ts",
  "version": "0.1.0",
  "scripts": {
    "dev": "vite",
    "build": "vuedx-typecheck . && vite build",
    "serve": "vite preview",
    "lint:eslint": "eslint \"src/**/*.{ts,tsx,js,json,vue}\" --fix",
    "lint:prettier": "prettier --write --loglevel warn \"src/**/*.{js,json,tsx,ts,css,scss,vue,html,md}\"",
    "lint:style": "stylelint \"**/*.{vue,css,scss,sass}\" \"!dist/**/*\"",
    "css-mj": "node-sass -r ./style/mj/mj.scss --include-path node_modules --output-style expanded --source-map true --source-map-contents true --precision 6 -o public/css",
    "css": "nodemon",
    "test": "jest"
  },
  "dependencies": {
    "babylonjs": "^4.2.0",
    "bootstrap": "^5.0.0",
    "dat.gui": "^0.7.7",
    "nodemon": "^2.0.7",
    "sass": "^1.32.8",
    "stylelint": "^13.12.0",
    "stylelint-config-standard": "^21.0.0",
    "stylelint-order": "^4.1.0",
    "three": "^0.127.0",
    "vue": "^3.0.5",
    "vue-router": "^4.0.3",
    "vuex": "^4.0.0"
  },
  "devDependencies": {
    "@popperjs/core": "^2.9.2",
    "@types/node": "^14.14.41",
    "@types/three": "^0.127.1",
    "@typescript-eslint/eslint-plugin": "^4.19.0",
    "@typescript-eslint/parser": "^4.19.0",
    "@vitejs/plugin-vue": "^1.2.1",
    "@vitejs/plugin-vue-jsx": "^1.1.0",
    "@vue/compiler-sfc": "^3.0.5",
    "@vue/test-utils": "^2.0.0-rc.4",
    "@vuedx/typecheck": "^0.6.0",
    "@vuedx/typescript-plugin-vue": "^0.6.0",
    "axios": "^0.21.1",
    "codemirror": "^5.61.0",
    "eslint": "^7.22.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-vue": "^7.8.0",
    "jest": "^26.6.3",
    "mockjs": "^1.1.0",
    "node-sass": "^5.0.0",
    "prettier": "^2.2.1",
    "ts-jest": "^26.5.4",
    "typescript": "^4.1.3",
    "vite": "^2.1.5",
    "vite-plugin-mock": "^2.5.0",
    "vue-eslint-parser": "^7.6.0",
    "vue-jest": "^5.0.0-alpha.7"
  }
}
